{
  "$id": "https://klarpen.com/schemas/PortType.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "PortType",
  "description": "Known charging port types descriptors",
  "type": "object",
  "properties": {
    "portTypeId": { "type": "integer" },
    "socket": { "type": "string" },
    "current": {
      "type": "string",
      "enum": ["DC", "AC"]
    }
  },
  "additionalProperties": false,
  "required": ["portTypeId", "socket", "current"]
}
